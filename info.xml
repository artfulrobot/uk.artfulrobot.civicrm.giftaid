<?xml version="1.0"?>
<extension key="uk.artfulrobot.civicrm.giftaid" type="module">
  <file>giftaid</file>
  <name>Gift Aid</name>
  <description>Simple gift aid extension.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott</author>
    <email>code.commits@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/uk.artfulrobot.civicrm.giftaid</url>
    <url desc="Documentation">https://artfulrobot.github.io/uk.artfulrobot.civicrm.giftaid/</url>
    <url desc="Support">https://github.com/artfulrobot/uk.artfulrobot.civicrm.giftaid/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-12-02</releaseDate>
  <version>1.7.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>6.5</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Giftaid</namespace>
    <format>25.01.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CiviMix\Schema\Giftaid\AutomaticUpgrader</upgrader>
  <mixins>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
